{
  "name": "myfitnesspal",
  "version": "1.0.2",
  "description": "Submit nutrition data to MyFitnessPal.",
  "main": "src/client.js",
  "type": "module",
  "scripts": {
    "prepack": "cp ../README.MD ../LICENSE ."
  },
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=22.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ViMaSter/myfitnesspal-api.git"
  },
  "author": "Vincent Mahnke (vincent@mahn.ke)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ViMaSter/myfitnesspal-api/issues"
  },
  "homepage": "https://github.com/ViMaSter/myfitnesspal-api#readme",
  "release": {
    "branches": [
      {
        "name": "main"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "semantic-release": "^24.2.5"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "jwa": "^2.0.1"
  }
}
